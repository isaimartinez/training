     "text": [
      "Write strhola\n",
      "str:  hola \n",
      "Reversed:  aloh\n"
     ]
    }
   ],
   "source": [
    "str = input(\"Write str :\")\n",
    "\n",
    "def reverseStr(s):\n",
    "    strs = []\n",
    "    i = len(s)\n",
    "    while i:\n",
    "        i -= 1                       \n",
    "        strs.append(s[i])\n",
    "    return ''.join(strs)\n",
    "\n",
    "print(\"str: \", str, \"\\nReversed: \", reverseStr(str))"
   ]
  }
